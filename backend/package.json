    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.2.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
